//rotate array kth place

class Solution {
    static void rotateArr(int arr[], int d) {
        // code here
       
        int k=d%arr.length;
         int temp[]= new int[k];
        for(int i=0;i<k;i++){
            temp[i]=arr[i];
        }
        for(int i=k;i<arr.length;i++){
            arr[i-k]=arr[i];
        }
        for(int i=arr.length-k;i<arr.length;i++){
            arr[i]=temp[i+k-arr.length];
        }
        
    }
}